{
  "name": "bluez-avrcp-rest",
  "version": "0.0.1",
  "description": "A rest service to control connected Bluetooth audio devices over AVRCP",
  "main": "src/main.js",
  "scripts": {
    "deploy": "rsync -rltzC --delete --exclude=node_modules ~/repos/personal/rpi/bluez-avrcp-rest rpi:~/",
    "debug:remote": "ssh -t -L 9229:localhost:9229 rpi \"sudo node --inspect-brk ~/bluez-avrcp-rest/main.js\"",
    "start:remote": "ssh -t rpi \"sudo node ~/bluez-avrcp-rest/main.js\"",
    "start": "node src/main.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vladimir-kotikov/bluez-avrcp-rest.git"
  },
  "keywords": [
    "bluez",
    "agent",
    "avrcp"
  ],
  "author": "Vladimir Kotikov",
  "license": "MIT",
  "dependencies": {
    "dbus-native": "^0.2.2"
  }
}
